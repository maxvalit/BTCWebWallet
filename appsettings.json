{
  "Kestrel": {
      "Endpoints": {
          "Https": {
              "Url": "https://localhost:9999"
          }
      }
  },
  "Logging": {
      "LogLevel": {
          "Default": "Error",
          "Microsoft": "Warning",
          "Microsoft.Hosting.Lifetime": "Warning"
      },
      "Debug": {
          "LogLevel": {
              "Default": "Information"
          }
      },
      "Console": {
          "IncludeScopes": true,
          "LogLevel": {
              "Microsoft.Extensions.Hosting": "Warning",
              "Default": "Information"
          }
      },
      "EventSource": {
          "LogLevel": {
              "Microsoft": "Information"
          }
      },
      "EventLog": {
          "LogLevel": {
              "Microsoft": "Information"
          }
      },
      "AzureAppServicesFile": {
          "IncludeScopes": true,
          "LogLevel": {
              "Default": "Warning"
          }
      },
      "AzureAppServicesBlob": {
          "IncludeScopes": true,
          "LogLevel": {
              "Microsoft": "Information"
          }
      },
      "ApplicationInsights": {
          "LogLevel": {
              "Default": "Information"
          }
      }
  },
  "RPC": {
      "rpcbind": "127.0.0.1",
      "rpcport": "18443",
      "rpcuser": "rpcuser",
      "rpcpassword": "rpcpassword",
      "rpcallowip": "127.0.0.1"
  },
  "WalletSettings": {
      "UnlockTimeout": 600
  },
  "BitcoinSettings": {
      "executablePath": "/usr/local/opt/bitcoin/bin/bitcoind",
      "configPath": "bitcoin_test.conf",
      "walletPath": "test_wallet",
      "dataPath": "test_data"
  }
}